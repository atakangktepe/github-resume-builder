{
  "name": "github-resume",
  "description": "Tool to build your online resume using the Github information.",
  "author": "Atakan Goktepe",
  "license": "MIT",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/barbar/react-github-resume"
  },
  "homepage": "https://github.com/barbar/react-github-resume",
  "keywords": [
    "react",
    "redux",
    "universal",
    "starter"
  ],
  "main": "bin/server.js",
  "scripts": {
    "start": "concurrently --kill-others \"npm run start-prod\"",
    "start-prod": "better-npm-run start-prod",
    "build": "better-npm-run build",
    "postinstall": "npm run build",
    "lint": "eslint -c .eslintrc src",
    "start-dev": "better-npm-run start-dev",
    "watch-client": "better-npm-run watch-client",
    "dev": "concurrently --kill-others \"npm run watch-client\" \"npm run start-dev\"",
    "test": "karma start",
    "test-node": "./node_modules/mocha/bin/mocha $(find api -name '*-test.js') --compilers js:babel-core/register",
    "test-node-watch": "./node_modules/mocha/bin/mocha $(find api -name '*-test.js') --compilers js:babel-core/register --watch"
  },
  "betterScripts": {
    "start-prod": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8080
      }
    },
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src",
        "PORT": 3000
      }
    },
    "build": {
      "command": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "dependencies": {
    "axios": "0.9.1",
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-react-display-name": "^6.5.0",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "babel-runtime": "^6.6.1",
    "body-parser": "^1.15.0",
    "classnames": "^2.2.3",
    "compression": "^1.6.1",
    "express": "^4.13.4",
    "express-session": "^1.13.0",
    "file-loader": "0.8.5",
    "hoist-non-react-statics": "^1.0.5",
    "http-proxy": "^1.13.2",
    "invariant": "^2.2.1",
    "less": "^2.6.1",
    "less-loader": "^2.2.3",
    "lru-memoize": "^1.0.1",
    "map-props": "1.0.0",
    "multireducer": "2.0.0",
    "piping": "0.3.0",
    "pretty-error": "2.0.0",
    "react": "15.0.1",
    "react-dom": "15.0.1",
    "react-helmet": "3.0.2",
    "react-inline-css": "^2.1.1",
    "react-redux": "^4.4.5",
    "react-router": "2.3.0",
    "react-router-redux": "3.0.0",
    "redux": "^3.5.1",
    "redux-async-connect": "1.0.0-rc4",
    "redux-form": "5.1.1",
    "redux-thunk": "^2.0.1",
    "scroll-behavior": "0.4.0",
    "serialize-javascript": "^1.2.0",
    "serve-favicon": "2.3.0",
    "url-loader": "0.5.7",
    "warning": "2.1.0",
    "webpack-isomorphic-tools": "^2.2.48"
  },
  "devDependencies": {
    "autoprefixer": "6.3.6",
    "babel-eslint": "6.0.3",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-typecheck": "^3.8.0",
    "better-npm-run": "0.0.8",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^0.1.8",
    "concurrently": "2.0.0",
    "css-loader": "0.23.1",
    "eslint": "2.8.0",
    "eslint-config-airbnb": "7.0.0",
    "eslint-loader": "1.3.0",
    "eslint-plugin-import": "1.5.0",
    "eslint-plugin-jsx-a11y": "1.0.2",
    "eslint-plugin-react": "5.0.1",
